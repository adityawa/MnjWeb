<div class="page-title">
    <div>
        <h1><i class="fa fa-edit"></i> Report</h1>
        <p>Inquiry Transaction By Therapis</p>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="row">
                <div class="panel-group" id="accordion">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">Parameters</a>
                            </h4>
                        </div>
                        <div id="collapseOne" class="panel-collapse collapse in">
                            <div class="panel-body">

                                <div class="bs-component">
                                    <form id="frm1" class="form-inline">
                                        <div class="form-group">
                                            <label for="exampleInputName2">Period</label>
                                            <input type="text" class="form-control" id="txtPeriodStart" placeholder="Start">
                                            <label for="exampleInputName2">~</label>
                                            <input type="text" class="form-control" id="txtPeriodEnd" placeholder="End">
                                        </div>
                                       
                                       
                                    </form>
                                  <br />
                                    <form id="frm3" class="form-inline">
                                        <div class="form-group">
                                            <label for="exampleInputEmail2">Theraphis</label>
                                            <select class="form-control" id="selTheraphis" name="selTheraphis" required>
                                                <option value="">ALL</option>
                                            </select>
                                        </div>
                                    </form>
                                    <br />
                                  <form id="frm2" class="form-inline">
                                      <div class="form-group">
                                          <label class="control-label " for="exampleInputEmail2">Gerai</label>

                                          <select class="form-control" id="selGerai" name="selGerai" required>
                                              <option value="">ALL</option>
                                          </select>
                                          <label class="control-label" for="exampleInputEmail2">Action</label>

                                          <select class="form-control" id="selÀction" name="selÀction" required>
                                              <option value="PREVIEW">Preview</option>
                                              <option value="PRINT">Print</option>
                                          </select>
                                          <button type="button" id="btnViewReport" class="btn-primary"><i class="fa fa-search" aria-hidden="true"></i> Search </button>
                                      </div>
                                  </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="grid" style="display:none" class="form-group">
                    <div class="col-md-12">
                        <table class="table table-hover table-bordered" id="gridResult">
                            <thead style="background-color:blue; color:white">
                                <tr>
                                    <th>SW</th>
                                    <th>Date</th>
                                    <th>Type</th>
                                    <th>Theraphis</th>
                                    <th>Non Voucher</th>
                                    <th>Prepaid Package</th>
                                    <th>Gift Voucher</th>
                                    <th>Grand Total</th>
                                    <th>Commision</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <iframe name="rpt" width="100%" height="500" id="rpt"></iframe>
            </div>
        </div>
    </div>
</div>
